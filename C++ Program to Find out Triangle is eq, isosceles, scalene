#include <iostream>

using namespace std;

int main()
{
    cout<<"Enter the three sides of your triangle:"<<endl;
    float l1,l2,l3;
    cin>>l1>>l2>>l3;
    if(l1==l2 && l2==l3 && l3==l1){
        cout<<"Your triangle is equilateral.";
    }
    else if(l1==l2 || l2==l3 || l3==l1){
        cout<<"Your triangle is isosceles.";
    }
    else{
        cout<<"Your triangle is scalene.";
    }
    
    
   return 0;
}
