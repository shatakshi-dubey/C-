#include <iostream>

using namespace std;

int main()
{
   int a,b,c;
   cout<<"Enter the three numbers:\n";
   cin>>a>>b>>c;
   if(a>b && a>c)
   cout<<"Largest number amongst the three is "<<a;
   if(b>a && b>c)
   cout<<"Largest number amongst the three is "<<b;
   if(c>a && c>b)
   cout<<"Largest number amongst the three is "<<c;
   
   return 0;
    
}
